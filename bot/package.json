{
  "name": "step-one-bot",
  "version": "1.0.0",
  "description": "Telegram bot for nutrition tracking",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "restart": "pm2 restart step-one-bot || pm2 start ecosystem.config.js",
    "health-check": "tsx ../scripts/health-check-bot.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "nutrition"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "dotenv": "^17.2.3",
    "openai": "^6.9.0",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}
